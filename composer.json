{
    "name": "obullo/mvc",
    "type": "library",
    "description": "Create micro mvc applications with Obullo and Zend components.",
    "keywords": [
        "obullo",
        "modular",
        "micro",
        "mvc",
        "framework",
        "psr-7",
        "psr-11",
        "psr-15"
    ],
    "homepage": "http://mvc.obullo.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Ersin Guvenc",
            "email": "eguvenc@gmail.com",
            "homepage": "http://obullo.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "" : "classes",
            "App\\": "bundle/App/src/",
            "View\\": "bundle/View/src/",
            "Obullo\\": "src/"
        }
    },
    "require": {
        "php": "^7.0",
    },
    "require-dev": {
        "obullo/stack": "^1.1",
        "obullo/router": "^1.0",
        "league/plates": "^3.3",
        "zendframework/zend-db": "^2.9",
        "symfony/console": "3.4.17",
        "zendframework/zend-config": "^3.1",
        "zendframework/zend-config-aggregator": "^1.1",
        "zendframework/zend-diactoros": "^1.7",
        "zendframework/zend-hydrator": "^2.4",
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-session": "^2.8",
        "zendframework/zend-i18n-resources": "^2.5",
        "zendframework/zend-i18n": "^2.7",
        "zendframework/zend-escaper": "^2.6",
        "monolog/monolog": "^1.23",
        "doctrine/dbal": "2.5.13",
        "phpunit/phpunit": "^4.8",
        "vlucas/phpdotenv": "^2.4"
    }
}
